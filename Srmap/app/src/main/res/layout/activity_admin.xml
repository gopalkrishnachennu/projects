<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".dashboard.Admin">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_margin="5dp"
        android:elevation="10dp"
        app:cardCornerRadius="20dp"
        app:cardElevation="10dp"
        android:fadingEdgeLength="10dp"
        android:layout_marginTop="15dp"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fitsSystemWindows="true"
            android:orientation="horizontal">



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="140dp"
                android:orientation="vertical"
                android:gravity="center_vertical"
                >
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:layout_marginTop="10dp"
                    android:text="Gopall Krishna Chennu"
                    android:textSize="20sp"
                    android:singleLine="true"
                    android:textStyle="bold"
                    android:textColor="@color/colorPrimary"
                    android:layout_marginStart="10dp"
                    />

                <TextView
                    android:id="@+id/gkmail"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:text="gopalakrishna@srmap.edu.in"
                    android:layout_marginStart="10dp"
                    android:textStyle="italic"
                    android:textIsSelectable="true"
                    android:maxLength="80"
                    android:maxLines="2"
                    />
                <TextView
                    android:id="@+id/stamp"
                    android:layout_width="match_parent"
                    android:layout_height="20sp"
                    android:gravity="end"
                    android:layout_marginEnd="30dp"
                    android:text="#SRM AP"
                    android:textStyle="bold"
                    android:textColor="#F63939"/>

            </LinearLayout>










        </LinearLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_margin="5dp"
        android:elevation="10dp"
        app:cardCornerRadius="20dp"
        app:cardElevation="10dp"
        android:fadingEdgeLength="10dp"
        android:layout_marginTop="15dp"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fitsSystemWindows="true"
            android:orientation="horizontal">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="140dp"
                android:orientation="vertical"
                android:gravity="center_vertical"
                >
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:layout_marginTop="10dp"
                    android:text="Harish Thota"
                    android:textSize="20sp"
                    android:singleLine="true"
                    android:textStyle="bold"
                    android:textColor="@color/colorPrimary"
                    android:layout_marginStart="10dp"
                    />

                <TextView
                    android:id="@+id/harishmail"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:text="thota_harish@srmap.edu.in"
                    android:layout_marginStart="10dp"
                    android:textStyle="italic"
                    android:textIsSelectable="true"
                    android:maxLength="80"
                    android:maxLines="2"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="20sp"
                    android:gravity="end"
                    android:layout_marginEnd="30dp"
                    android:text="#SRM AP"
                    android:textStyle="bold"
                    android:textColor="#F63939"/>

            </LinearLayout>










        </LinearLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_margin="5dp"
        android:elevation="10dp"
        app:cardCornerRadius="20dp"
        app:cardElevation="10dp"
        android:fadingEdgeLength="10dp"
        android:layout_marginTop="15dp"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fitsSystemWindows="true"
            android:orientation="horizontal">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="140dp"
                android:orientation="vertical"
                android:gravity="center_vertical"
                >
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:layout_marginTop="10dp"
                    android:text="Sai Saran Yadlapalli"
                    android:textSize="20sp"
                    android:singleLine="true"
                    android:textStyle="bold"
                    android:textColor="@color/colorPrimary"
                    android:layout_marginStart="10dp"
                    />

                <TextView
                    android:id="@+id/saimail"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:text="yadlapalli_saisaran@srmap.edu.in"
                    android:layout_marginStart="10dp"
                    android:textStyle="italic"
                    android:textIsSelectable="true"
                    android:maxLength="80"
                    android:maxLines="2"
                    />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="20sp"
                    android:gravity="end"
                    android:layout_marginEnd="30dp"
                    android:text="#SRM AP"
                    android:textStyle="bold"
                    android:textColor="#F63939"/>

            </LinearLayout>










        </LinearLayout>

    </androidx.cardview.widget.CardView>






</LinearLayout>